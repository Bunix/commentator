{
    "name": "plmrlnsnts/commentator",
    "description": "A Laravel package that lets you add a comment section to your pages.",
    "license": "MIT",
    "authors": [
        {
            "name": "Paul Santos",
            "email": "paulmarlonsantos@gmail.com",
            "homepage": "https://github.com/plmrlnsnts"
        }
    ],
    "homepage": "https://github.com/plmrlnsnts/commentator",
    "keywords": ["Laravel", "Commentator"],
    "require": {
        "livewire/livewire": "^1.0",
        "illuminate/support": "~5.8|~6.0|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Plmrlnsnts\\Commentator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Plmrlnsnts\\Commentator\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Plmrlnsnts\\Commentator\\CommentatorServiceProvider"
            ],
            "aliases": {
                "Commentator": "Plmrlnsnts\\Commentator\\Facades\\Commentator"
            }
        }
    }
}
